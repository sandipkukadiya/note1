    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
  body(ng-app='frontend', ng-clock='')
    // Navigation
    nav.navbar.navbar-default.navbar-custom.navbar-fixed-top
      .container-fluid
        // Brand and toggle get grouped for better mobile display
        .navbar-header.page-scroll
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
            span.sr-only Toggle navigation
            |                     Menu 
            i.fa.fa-bars
          a.navbar-brand(href='index.html') Start Bootstrap
        // Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
          ul.nav.navbar-nav.navbar-right
            li
              a(href='index.html') Home
            li
              a(href='about.html') About
            li
              a(href='post.html') Sample Post
            li
              a(href='contact.html') Contact
        // /.navbar-collapse
      // /.container
    // Page Header
    // Set your background image for this header on the line below.
    header.intro-header(style="background-image: url('/theme/img/home-bg.jpg')")
      .container
        .row
          .col-lg-8.col-lg-offset-2.col-md-10.col-md-offset-1
            .site-heading
              h1 Clean Blog
              hr.small
              span.subheading A Clean Blog Theme by Start Bootstrap

    block content

    footer
        .container
            .row
              .col-lg-8.col-lg-offset-2.col-md-10.col-md-offset-1
                ul.list-inline.text-center
                  li
                    a(href='#')
                      span.fa-stack.fa-lg
                        i.fa.fa-circle.fa-stack-2x
                        i.fa.fa-twitter.fa-stack-1x.fa-inverse
                  li
                    a(href='#')
                      span.fa-stack.fa-lg
                        i.fa.fa-circle.fa-stack-2x
                        i.fa.fa-facebook.fa-stack-1x.fa-inverse
                  li
                    a(href='#')
                      span.fa-stack.fa-lg
                        i.fa.fa-circle.fa-stack-2x
                        i.fa.fa-github.fa-stack-1x.fa-inverse
                p.copyright.text-muted Copyright Â© Your Website 2016
    
    script(src='/scripts/angular/angular.js')
    script(src='/scripts/angular-route/angular-route.min.js')
    script(src='/frontend/angular-module.js')
    // jQuery
    script(src='/theme/vendor/jquery/jquery.min.js')
    // Bootstrap Core JavaScript
    script(src='/theme/vendor/bootstrap/js/bootstrap.min.js')
    // Contact Form JavaScript
    script(src='/theme/js/jqBootstrapValidation.js')
    script(src='/theme/js/contact_me.js')
    // Theme JavaScript
    script(src='/theme/js/clean-blog.min.js')
